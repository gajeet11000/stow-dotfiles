configuration
{
	modi: "Home:~/.config/rofi/scripts/terminal-home.sh, Storage:~/.config/rofi/scripts/terminal-storage.sh, Root:~/.config/rofi/scripts/terminal-root.sh";
	font: "Comic Sans MS Bold 14";
	show-icons: true;
	icon-theme: "candy-icons";
	matching: "normal";
	sort: true;
	sorting-method: "fzf";
	case-sensitive: false;
	hover-select: false;
	me-select-entry: "";
	me-accept-entry: [ MousePrimary ];
}
@theme "~/.config/rofi/themes/search.rasi"